extends base.pug
block style 
    style 
        include ../static/style.css 
        include ../static/contact.css
block content
    div.container
        h1 Contact Us
            form(action="/contact" method="post" class="myForm")
                input(type="text" name="name" class="myInput" placeholder="Enter Your Name :") 
                input(type="text" name="age" class="myInput" placeholder="Enter Your Age :") 
                input(type="text" name="gender" class="myInput" placeholder="Enter Your gender :") 
                input(type="text" name="phone" class="myInput" placeholder="Enter Your phone number :")
                input(type="text" name="email" class="myInput" placeholder="Enter Your email :")
                button(class="btn")
                    | Submit
            
   
    